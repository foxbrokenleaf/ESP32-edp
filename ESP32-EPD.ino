        if(buff[i + 3] == ','){
          for(uint8_t j = 0;j < 16;j++){
            if(buff[i + 2] == hex_code_char[j]){
                Write_Data(hex_code_num[j]);
            }
          }
      }
      else{
          uint8_t temp = 0;
          for(uint8_t j = 0;j < 16;j++){
            if(buff[i + 2] == hex_code_char[j]){
              temp = hex_code_num[j] * 16;
            }
          }
          for(uint8_t j = 0;j < 16;j++){
            if(buff[i + 3] == hex_code_char[j]){
              temp += hex_code_num[j];
            }
          }
          Write_Data(temp);
        }
      }
    }
    Write_Command(0x13);  
    for(uint16_t i = 0;i < 296;i++){
      for(uint16_t j = 0;j < 152 / 8;j++){
        Write_Data(0x00);
      }
    }
  SRAM_Mode();  
}

unsigned int Ses_EDP::EDP_square(unsigned int num,unsigned char square){
    unsigned int rt_dat = num;
    while(square--) rt_dat *= num;
    return rt_dat;
}

void Ses_EDP::EDP_ShowChar(int column,int line,char dat){
    unsigned char font_index = 0;
    if(dat == 32) font_index = 0;
    else if(dat >= 48 && dat <= 57)  font_index = dat - 47;
    else if(dat >= 65 && dat <= 90)  font_index = dat - 28;
    else if(dat >= 97 && dat <= 122)  font_index = dat - 86;
    for(int y = line,x = 0;x < 15 && y < 296 && column <= 19;x++,y++){
        EDP_Buff[y][column] = EDP_Font[font_index][x];
    }
}

void Ses_EDP::EDP_ShowString(int column,int line,const char *dat){
    unsigned char dat_index = 0;
    while(*(dat) != '\0') EDP_ShowChar(column++,line,*(dat++));
}

void Ses_EDP::EDP_ShowNum(int column,int line,unsigned int num,unsigned char len){
    unsigned char i = 0;
    for(i = len;i >= 0;i--,column++){
        if(i == len) EDP_ShowChar(column,line,num % 10 + 48);
        if(i == 0) EDP_ShowChar(column,line,num / EDP_square(10,len - 1) % 10 + 48);
    }
    EDP_ShowChar(column++,1,1234 / 1000 + 48);
    EDP_ShowChar(column++,1,1234 / 100 % 10 + 48);
    EDP_ShowChar(column++,1,1234 / 10 % 10 + 48);
    EDP_ShowChar(column++,1,1234 % 10 + 48);
}
