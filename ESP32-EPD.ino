Ses_EDP::Ses_EDP(){
  pinMode(BUSY, INPUT_PULLUP);
  pinMode(RST, OUTPUT);
  pinMode(DC, OUTPUT);
  pinMode(CS, OUTPUT);
  pinMode(SCL, OUTPUT);
  pinMode(SDA, OUTPUT);

  RST_Write(1);
  DC_Write(1);
  CS_Write(1);
  SCL_Write(1);
  SDA_Write(1);

  Display_Init_Office();
}
void Ses_EDP::BUSY_Wait(){
  Serial.print("Waiting BUSY");
  Serial.flush();
  while(digitalRead(BUSY) != HIGH){
    Serial.print(".");
    delay(500);
  }
  Serial.print("\n\r");
  Serial.flush();
}
void Ses_EDP::Write_Data(uint8_t dat){
  CS_Write(0);
  DC_Write(1);
  for(int i = 0;i < 8;i++){
    SCL_Write(0);
    SDA_Write(dat & (0x80 >> i));
    SCL_Write(1);
    SCL_Write(0);
  }
  CS_Write(1);
}
void Ses_EDP::Write_Command(uint8_t command){
  CS_Write(0);
  DC_Write(0);
  for(int i = 0;i < 8;i++){
    SCL_Write(0);
    SDA_Write(command & (0x80 >> i));
    SCL_Write(1);
    SCL_Write(0);
  }
  CS_Write(1);
}
//硬复位
void Ses_EDP::COG_Reset(){
  delay(5);
  RST_Write(1);
  delay(5);
  RST_Write(0);
  delay(10);
  RST_Write(1);
  delay(5);
  CS_Write(1);
  delay(5);
}
void Ses_EDP::SRAM_Mode(){
  delay(50);
  Write_Command(POWER_ON);
  Write_Command(POWER_ON);
  delay(5);
  BUSY_Wait();
  Write_Command(DISPLAY_REFRESH);
  Write_Command(DISPLAY_REFRESH);
  delay(5);
  BUSY_Wait();
  Write_Command(TURN_OFF_DC);
  Write_Command(TURN_OFF_DC);
  BUSY_Wait();

  DC_Write(0);
  CS_Write(0);
  RST_Write(0);
  CS_Write(1);
}
void Ses_EDP::Display_Init_Office(){
  COG_Reset();

  Write_Command(SOFT_RESET); Write_Data(0xE0);
  delay(5);

  Write_Command(WRITE_TEMPERATURE); Write_Data(0x19);
  Write_Command(APPLAY_TEMPERATURE); Write_Data(0x02);
  Write_Command(PSR); Write_Data(0xCF); Write_Data(0x8D);
}
void Ses_EDP::Display_Clearn(){
  Write_Command(0x10);
  for(uint16_t i = 0;i < 296;i++){
    for(uint16_t j = 0;j < 152 / 8;j++){
      Write_Data(0x00);
    }
  }
  Write_Command(0x13);
  for(uint16_t i = 0;i < 296;i++){
    for(uint16_t j = 0;j < 152 / 8;j++){
      Write_Data(0x00);
    }
  }
  SRAM_Mode();
}

void Ses_EDP::DrawImage(){
  uint16_t img_pos = 0;
  Display_Init_Office();
  Write_Command(0x10);
  for(uint16_t i = 0;i < 296;i++){
    for(uint16_t j = 0;j < 152 / 8;j++){
      Write_Data(EDP_Buff[i][j]);
    }
  }
  Write_Command(0x13);
  for(uint16_t i = 0;i < 296;i++){
    for(uint16_t j = 0;j < 152 / 8;j++){
      Write_Data(0x00);
    }
  }
  SRAM_Mode();
}

